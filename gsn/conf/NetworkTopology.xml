<binding>
    <mapping name="network" class="gsn.beans.NetworkTopology">
       <collection name="sensornodes" field="sensornodes"  item-type="gsn.beans.SensorNode" />
       <value name="configurable" field="configurable" style="attribute" usage="optional"/>
       <value name="mapped" field="mapped" style="attribute" usage="optional"/>
   </mapping>

   <mapping name="sensornode" class="gsn.beans.SensorNode" >   
  	   <value name="node_id" field="node_id" style="attribute"/>
       <value name="position" field="position" style="attribute" usage="optional"/>
       <value name="longitude" style="attribute" get-method="getCoordinateLongitude" set-method="setCoordinateLongitude" usage="optional"/>      
       <value name="latitude" style="attribute" get-method="getCoordinateLatitude" set-method="setCoordinateLatitude" usage="optional"/>
       <value name="altitude" style="attribute" get-method="getCoordinateAltitude" set-method="setCoordinateAltitude" usage="optional"/>
  	   <value name="parent_id" field="parent_id" style="attribute" usage="optional"/>
  	   <value name="timestamp" field="timestamp" style="attribute" usage="optional"/>
  	   <value name="generation_time" field="generation_time" style="attribute" usage="optional"/>
  	   
  	   <value name="nodetype" field="nodetype" style="attribute" usage="optional"/>
  	   <value name="packet_count" field="packet_count" style="attribute" usage="optional"/>
  	   <value name="vsys" style="attribute" usage="optional" get-method="getVsys" set-method="setVsys"/>
  	   <value name="vsdi" style="attribute" usage="optional" get-method="getVsdi" set-method="setVsdi"/>
  	   <value name="current" style="attribute" usage="optional" get-method="getCurrent" set-method="setCurrent"/>
  	   <value name="temperature" style="attribute" usage="optional" get-method="getTemperature" set-method="setTemperature"/>
  	   <value name="humidity" style="attribute" usage="optional" get-method="getHumidity" set-method="setHumidity"/>
  	   <value name="flash_count" field="flash_count" style="attribute" usage="optional"/>
  	   <value name="uptime" field="uptime" style="attribute" usage="optional"/>
  	   <value name="batterylevel" style="attribute" get-method="getBatteryLevel" set-method="setBatteryLevel" usage="optional"/>
  	   <value name="corestation_online" field="corestation_online" style="attribute" usage="optional"/>
  	   <value name="iscorestation" field="iscorestation" style="attribute" usage="optional"/>
  	   <value name="iswgpsv2" field="iswgpsv2" style="attribute" usage="optional"/>
  	   <value name="db_entries" field="db_entries" style="attribute" usage="optional"/>
  	   
  	   <collection name="links" field="links" item-type="gsn.beans.Link" />
  	   
  	   <structure field="configuration" name="configuration" usage="optional" />
       <structure field="pendingConfiguration" name="pendingConfiguration" usage="optional" />
       
   </mapping>
   
   <mapping name="link" class="gsn.beans.Link" >
  	   <value name="node_id" field="node_id" style="attribute"/>
  	   <value name="rssi" field="rssi" style="attribute"/>
  	   <value name="timestamp" field="timestamp" style="attribute"/>
   </mapping>
   
   <mapping abstract="true" class="gsn.beans.SensorNodeConfiguration" >
   	<value name="info" field="info" style="attribute" usage="optional"/>
	<value name="health" field="health" style="attribute" usage="optional"/>
	<value name="adcmux" field="adcmux" style="attribute" usage="optional"/>
	<value name="adccomdiff" field="adccomdiff" style="attribute" usage="optional"/>
	<value name="dcx" field="dcx" style="attribute" usage="optional"/>
	<value name="drift" field="drift" style="attribute" usage="optional"/>
	<value name="events" field="events" style="attribute" usage="optional"/>
	<value name="rssi" field="rssi" style="attribute" usage="optional"/>
	<value name="statecounter" field="statecounter" style="attribute" usage="optional"/>
	<value name="decagonmux" field="decagonmux" style="attribute" usage="optional"/>
	<value name="powerswitch" field="powerswitch" style="attribute" usage="optional"/>
	<value name="powerswitch_p1" field="powerswitch_p1" style="attribute" usage="optional"/>
	<value name="powerswitch_p2" field="powerswitch_p2" style="attribute" usage="optional"/>
	<value name="vaisala_wxt520" field="vaisala_wxt520" style="attribute" usage="optional"/>
	<value name="th3" field="th3" style="attribute" usage="optional"/>
	<value name="enviroscan" field="enviroscan" style="attribute" usage="optional"/>
	<value name="modbus" field="modbus" style="attribute" usage="optional"/>
	<value name="wgps" field="wgps" style="attribute" usage="optional"/>
	<value name="timestamp" field="timestamp" style="attribute" usage="optional"/>
   </mapping>
   
   
</binding>
