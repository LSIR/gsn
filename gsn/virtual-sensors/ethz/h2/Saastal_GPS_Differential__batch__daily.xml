<virtual-sensor name="Saastal_GPS_Differential__batch__daily" priority="10" >
    <processing-class>
        <class-name>gsn.vsensor.ConditionalDeleteVirtualSensor</class-name>
        <unique-timestamps>false</unique-timestamps>
        <init-params>
            <param name="field1">POSITION</param>
            <param name="operation1">=</param>
            <param name="join2">AND</param>
            <param name="field2">GENERATION_TIME</param>
            <param name="operation2">=</param>
        </init-params>
        <output-structure>
            <field name="POSITION" type="INTEGER" index="true" />
			<field name="LABEL" type="VARCHAR(4)" />
            <field name="DEVICE_ID" type="INTEGER" />
            <field name="GENERATION_TIME" type="BIGINT" unit="unixtime" index="true"/>
			<field name="PROCESSING_TIME" type="BIGINT" unit="unixtime" />
			<field name="SENSOR_TYPE" type="VARCHAR(16)" />
			<field name="VERSION" type="SMALLINT" />
			<field name="REFERENCE_LABEL" type="VARCHAR(4)" />
			
			<field name="E" type="DOUBLE" unit="m" />
			<field name="N" type="DOUBLE" unit="m" />
			<field name="H" type="DOUBLE" unit="m" />
			<field name="SD_E" type="DOUBLE" unit="m" />
			<field name="SD_N" type="DOUBLE" unit="m" />
			<field name="SD_H" type="DOUBLE" unit="m" />
			
			<field name="V_E" type="DOUBLE" unit="cm/d" />
			<field name="V_N" type="DOUBLE" unit="cm/d" />
			<field name="V_H" type="DOUBLE" unit="cm/d" />
			<field name="V_HORIZ" type="DOUBLE" unit="cm/d" />
			<field name="V_3D" type="DOUBLE" unit="cm/d" />
			<field name="SD_V_E" type="DOUBLE" unit="cm/d" />
			<field name="SD_V_N" type="DOUBLE" unit="cm/d" />
			<field name="SD_V_H" type="DOUBLE" unit="cm/d" />
			<field name="SD_V_HORIZ" type="DOUBLE" unit="cm/d" />
			<field name="SD_V_3D" type="DOUBLE" unit="cm/d" />
			
			<field name="RATIO_OF_FIXED_AMBIGUITIES" type="DOUBLE" unit="%" />
        </output-structure>
    </processing-class>
    <description>Daily Differential GPS solutions from batch processing (only newest data points) from Saastal deployment</description>
    <life-cycle pool-size="10" />
    <addressing />
    <storage />
    <streams>
        <stream name="data">
            <source alias="source" storage-size="1" sampling-rate="1">
                <address wrapper="local">
                    <predicate key="query">select * from Saastal_GPS_Differential__batch</predicate>
                    <predicate key="start-time">continue</predicate>
                </address>
                <query> select * from wrapper </query>
            </source>
            <query> select * from source </query>
        </stream>
    </streams>
</virtual-sensor>
