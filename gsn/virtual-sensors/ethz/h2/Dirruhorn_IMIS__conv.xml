<virtual-sensor name="Dirruhorn_IMIS__conv" priority="10" >
    <processing-class>
        <class-name>gsn.vsensor.CSVParserVirtualSensor</class-name>
        <unique-timestamps>false</unique-timestamps>
        <init-params>
            <param name="position_mapping" />
            <param name="fields">SLF_LABEL,SLF_NR,GENERATION_TIME,TA,RH,VW,VW_MAX,DW,HS,HNW,PSUM,TSG,TSS,RSWR</param>
            <param name="formats">string,numeric,timestamp(y-M-d H:m:s),numeric,numeric,numeric,numeric,numeric,numeric,numeric,numeric,numeric,numeric,numeric</param>
            <param name="timezone">CET</param>
            <param name="skip-first-lines">1</param>
            <param name="bad-values"></param>
        </init-params>
        <output-structure>
            <field name="POSITION" type="INTEGER" index="true" />
            <field name="DEVICE_ID" type="INTEGER" />
            <field name="GENERATION_TIME" type="BIGINT" unit="unixtime" index="true"/>
			<field name="SENSOR_TYPE" type="VARCHAR(32)" />
            
            <field name="SLF_LABEL" type="VARCHAR(16)" />
            <field name="SLF_NR" type="INTEGER" />
            
            <field name="TA" type="DOUBLE" unit="째C" />
            <field name="RH" type="DOUBLE" unit="%" />
            <field name="VW" type="DOUBLE" unit="m/s" />
            <field name="VW_MAX" type="DOUBLE" unit="m/s" />
            <field name="DW" type="DOUBLE" unit="째" />
            <field name="HS" type="DOUBLE" unit="cm" />
            <field name="HNW" type="DOUBLE" unit="mm/h" />
            <field name="PSUM" type="DOUBLE" unit="mm" />
            <field name="TSG" type="DOUBLE" unit="째C" />
            <field name="TSS" type="DOUBLE" unit="째C" />
            <field name="RSWR" type="DOUBLE" unit="w/m^2" />
            
            <field name="FILE" type="VARCHAR(255)"/>
            <field name="SIZE" type="BIGINT" unit="bytes"/>
        </output-structure>
    </processing-class>
    <description>Converted IMIS weather station data from Dirruhorn deployment</description>
    <life-cycle pool-size="10" />
    <addressing />
    <storage storage-directory="/home/perma/permasense_vault/gsn-test-binaries/" />
    <streams>
        <stream name="data">
            <source alias="source" storage-size="1" sampling-rate="1">
                <address wrapper="local">
                    <predicate key="query"> select * from Dirruhorn_IMIS__files__mapped</predicate>
                    <predicate key="start-time">continue</predicate>
                </address>
                <query> select TIMED, DEVICE_ID, SENSOR_TYPE, RELATIVE_FILE1 as FILE, SIZE_FILE1 as SIZE from wrapper </query>
            </source>
            <query> select * from source </query>
        </stream>
    </streams>
</virtual-sensor>
