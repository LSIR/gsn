<virtual-sensor name="ETZ_GPS_Logger__data__mapped" priority="10" >
    <processing-class>
        <class-name>gsn.vsensor.GPSLoggerDataParser</class-name>
        <unique-timestamps>false</unique-timestamps>
        <init-params>
            <param name="position_mapping" />
            <param name="stream_type">raw-status</param>
        </init-params>
        <output-structure>
            <field name="POSITION" type="INTEGER" index="true" />
            <field name="DEVICE_ID" type="INTEGER" />
            <field name="GENERATION_TIME" type="BIGINT" unit="unixtime" index="true"/>
            <field name="TIMESTAMP" type="BIGINT" unit="unixtime" index="true"/>
            <field name="GSN_TIMESTAMP" type="BIGINT" unit="unixtime" index="true" />
            
			<field name="DEVICE_TYPE" type="VARCHAR(32)" />
		    <field name="DATA_TYPE" type="TINYINT"/>
		    <field name="SAMPLE_COUNT" type="INTEGER"/>
            <field name="GPS_RAW_DATA_VERSION" type="SMALLINT"/>
		    <field name="GPS_SATS" type="INTEGER"/>
			<field name="STATUS_V12DC_IN" type="INTEGER" unit="mV"/>
			<field name="STATUS_HUMIDITY" type="INTEGER" unit="%"/>
			<field name="STATUS_TEMPERATURE" type="SMALLINT" unit="Â°C"/>
			<field name="STATUS_INCLINOMETER_X" type="SMALLINT"/>
			<field name="STATUS_INCLINOMETER_Y" type="SMALLINT"/>
            <field name="RAW_DATA" type="binary"/>
            
        </output-structure>
    </processing-class>
    <description>GPS Logger parsed raw and status information from ETZ deployment</description>
    <life-cycle pool-size="10" />
    <addressing />
    <storage storage-directory="/home/perma/permasense_vault/gsn-test-binaries/" />
    <streams>
        <stream name="data">
            <source alias="source" storage-size="1" sampling-rate="1">
                <address wrapper="local">
                    <predicate key="query">select * from ETZ_GPS_Logger__files__mapped</predicate>
                    <predicate key="start-time">continue</predicate>
                </address>
                <query> select * from wrapper </query>
            </source>
            <query> select * from source </query>
        </stream>
    </streams>
</virtual-sensor>
