<virtual-sensor name="Dirruhorn_Dozer_GPSData" priority="10" >
    <processing-class>
        <class-name>gsn.vsensor.BridgeVirtualSensorPermasense</class-name>
        <unique-timestamps>false</unique-timestamps>
        <output-structure>
            <field name="DEVICE_ID" type="INTEGER" />
            <field name="GENERATION_TIME" type="BIGINT" unit="unixtime"/>
            <field name="TIMESTAMP" type="BIGINT" unit="unixtime"/>
            <field name="HEADER_SEQNR" type="INTEGER" />
            <field name="HEADER_ORIGINATORID" type="INTEGER" />
            <field name="HEADER_ATIME" type="INTEGER" unit="s"/>
	        <field name="PAYLOAD_TIMEOFWEEKMS" type="INTEGER"/>
      	    <field name="PAYLOAD_WEEKNUMBER" type="INTEGER"/>
      	    <field name="PAYLOAD_CARRIERPHASE_DOUBLE_0" type="SMALLINT"/>
      	    <field name="PAYLOAD_CARRIERPHASE_DOUBLE_1" type="SMALLINT"/>
      	    <field name="PAYLOAD_CARRIERPHASE_DOUBLE_2" type="SMALLINT"/>
      	    <field name="PAYLOAD_CARRIERPHASE_DOUBLE_3" type="SMALLINT"/>
