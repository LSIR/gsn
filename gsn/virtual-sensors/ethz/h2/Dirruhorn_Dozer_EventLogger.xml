<virtual-sensor name="Dirruhorn_Dozer_EventLogger" priority="10" >
    <processing-class>
        <class-name>gsn.vsensor.BridgeVirtualSensorPermasense</class-name>
        <unique-timestamps>false</unique-timestamps>
        <output-structure>
            <field name="DEVICE_ID" type="INTEGER" />
            <field name="GENERATION_TIME" type="BIGINT" unit="unixtime" index="true"/>
            <field name="TIMESTAMP" type="BIGINT" unit="unixtime"/>
            <field name="HEADER_SEQNR" type="INTEGER" />
            <field name="HEADER_ORIGINATORID" type="INTEGER" />
            <field name="HEADER_ATIME" type="INTEGER" unit="s"/>
            <field name="PAYLOAD_EVENTLOG_TIME0" type="INTEGER" unit="s"/>
            <field name="PAYLOAD_EVENTLOG_TIME1" type="INTEGER" unit="s"/>
            <field name="PAYLOAD_EVENTLOG_TIME2" type="INTEGER" unit="s"/>
            <field name="PAYLOAD_EVENTLOG_TIME3" type="INTEGER" unit="s"/>
            <field name="PAYLOAD_EVENTLOG_TIME4" type="INTEGER" unit="s"/>
            <field name="PAYLOAD_EVENTLOG_TIME5" type="INTEGER" unit="s"/>
            <field name="PAYLOAD_EVENTLOG_TIME6" type="INTEGER" unit="s"/>
            <field name="PAYLOAD_EVENTLOG_ID0" type="SMALLINT" />
            <field name="PAYLOAD_EVENTLOG_ID1" type="SMALLINT" />
            <field name="PAYLOAD_EVENTLOG_ID2" type="SMALLINT" />
            <field name="PAYLOAD_EVENTLOG_ID3" type="SMALLINT" />
            <field name="PAYLOAD_EVENTLOG_ID4" type="SMALLINT" />
            <field name="PAYLOAD_EVENTLOG_ID5" type="SMALLINT" />
            <field name="PAYLOAD_EVENTLOG_ID6" type="SMALLINT" />
            <field name="PAYLOAD_EVENTLOG_TYPE0" type="INTEGER" />
            <field name="PAYLOAD_EVENTLOG_TYPE1" type="INTEGER" />
            <field name="PAYLOAD_EVENTLOG_TYPE2" type="INTEGER" />
            <field name="PAYLOAD_EVENTLOG_TYPE3" type="INTEGER" />
            <field name="PAYLOAD_EVENTLOG_TYPE4" type="INTEGER" />
            <field name="PAYLOAD_EVENTLOG_TYPE5" type="INTEGER" />
            <field name="PAYLOAD_EVENTLOG_TYPE6" type="INTEGER" />
            <field name="PAYLOAD_EVENTLOG_VALUE0" type="INTEGER" />
            <field name="PAYLOAD_EVENTLOG_VALUE1" type="INTEGER" />
            <field name="PAYLOAD_EVENTLOG_VALUE2" type="INTEGER" />
            <field name="PAYLOAD_EVENTLOG_VALUE3" type="INTEGER" />
            <field name="PAYLOAD_EVENTLOG_VALUE4" type="INTEGER" />
            <field name="PAYLOAD_EVENTLOG_VALUE5" type="INTEGER" />
            <field name="PAYLOAD_EVENTLOG_VALUE6" type="INTEGER" />
            <field name="DATA_IMPORT_SOURCE" type="SMALLINT" />
        </output-structure>
    </processing-class>
    <description>Dozer messages (DozerEventLogMsg) from Dirruhorn deployment</description>
    <life-cycle pool-size="10" />
    <addressing />
    <storage />
    <streams>
        <stream name="data">
            <source alias="source" storage-size="1" sampling-rate="1">
                <address wrapper="backlog">
                    <predicate key="remote-connection">permasense-dh-bs01.ethz.ch:9003</predicate>
                    <predicate key="plugin-classname">gsn.wrappers.backlog.plugins.MigMessagePlugin</predicate>
                    <predicate key="message-classname">ch.ethz.permasense.tinyos2x.messages.DozerEventLoggerDecodedMsg</predicate>
                </address>
                <query> select * from wrapper </query>
            </source>
            <query> select * from source </query>
        </stream>
        <stream name="data0">
            <source alias="source" storage-size="1" sampling-rate="1">
                <address wrapper="backlog">
                    <predicate key="remote-connection">permasense-rd-bs01.ethz.ch:9003</predicate>
                    <predicate key="plugin-classname">gsn.wrappers.backlog.plugins.MigMessagePlugin</predicate>
                    <predicate key="message-classname">ch.ethz.permasense.tinyos2x.messages.DozerEventLoggerDecodedMsg</predicate>
                </address>
                <query> select * from wrapper </query>
            </source>
            <query> select * from source </query>
        </stream>
        <stream name="data1">
            <source alias="source" storage-size="1" sampling-rate="1">
                <address wrapper="backlog">
                    <predicate key="remote-connection">permasense-gg-bs01.ethz.ch:9003</predicate>
                    <predicate key="plugin-classname">gsn.wrappers.backlog.plugins.MigMessagePlugin</predicate>
                    <predicate key="message-classname">ch.ethz.permasense.tinyos2x.messages.DozerEventLoggerDecodedMsg</predicate>
                </address>
                <query> select * from wrapper </query>
            </source>
            <query> select * from source </query>
        </stream>

        <stream name="data6">
            <source alias="source" storage-size="1" sampling-rate="1">
                <address wrapper="backlog">
                    <predicate key="remote-connection">permasense-bh-bs01.ethz.ch:9003</predicate>
                    <predicate key="plugin-classname">gsn.wrappers.backlog.plugins.MigMessagePlugin</predicate>
                    <predicate key="message-classname">ch.ethz.permasense.tinyos2x.messages.DozerEventLoggerDecodedMsg</predicate>
                </address>
                <query> select * from wrapper </query>
            </source>
            <query> select * from source </query>
        </stream>
        </streams>
</virtual-sensor>
