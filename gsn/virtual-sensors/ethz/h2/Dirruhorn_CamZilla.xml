<virtual-sensor name="Dirruhorn_CamZilla" priority="10" >
    <processing-class>
        <class-name>gsn.vsensor.BridgeVirtualSensorPermasense</class-name>
        <unique-timestamps>false</unique-timestamps>
        <web-input password="test">
            <command name="picture_now">
                <field name="core_station" type="*text" defaultvalue="65535">the device_id of the receiving core station</field>
                <field name="camera" type="select:DSLR,1|Webcam,2">select camera</field>
                <field name="image_quality" type="select:JPEG Basic,0|JPEG Normal,1|JPEG Fine,2|TIFF (RGB),3|NEF (Raw),4|NEF+Basic,5|NEF+Normal,6|NEF+Fine,7">select image quality</field>
                <field name="image_size" type="select:2144x1424,2|3216x2136,1|4288x2848,0">select image size</field>
                <field name="aperture" type="select:auto|f/5.6,0|f/6.3,1|f/7.1,2|f/8,3|f/9,4|f/10,5|f/11,6|f/13,7|f/14,8|f/16,9|f/18,10|f/20,11|f/22,12|f/25,13|f/29,14|f/32,15|f/36,16">select aperture</field>
                <field name="shutter_speed" type="select:auto|0.0001s,0|0.0002s,1|0.0003s,2|0.0004s,3|0.0005s,4|0.0006s,5|0.0008s,6|0.0010s,7|0.0012s,8|0.0015s,9|0.0020s,10|0.0025s,11|0.0031s,12|0.0050s,13|0.0050s,14|0.0062s,15|0.0080s,16|0.0100s,17|0.0125s,18|0.0166s,19|0.0200s,20|0.0250s,21|0.0333s,22|0.0400s,23|0.0500s,24|0.0666s,25|0.0769s,26|0.1000s,27|0.1250s,28|0.1666s,29|0.2000s,30|0.2500s,31|0.3333s,32|0.4000s,33|0.5000s,34|0.6250s,35|0.7692s,36|1.0000s,37|1.300s,38|1.6000s,39|2.0000s,40|2.5000s,41|3.0000s,42|4.0000s,43|5.0000s,44|6.0000s,45|8.0000s,46|10.0000s,47|13.0000s,48|15.0000s,49|20.0000s,50|25.0000s,51|30.0000s,52">select shutter speed</field>
                <field name="iso" type="select:auto|100,0|125,1|160,2|200,3|250,4|320,5|400,6|500,7|640,8|800,9|1000,10|1250,11|1600,12|2000,13|2500,14|3200,15|4000,16|5000,17|6400,18">select ISO</field>
                <field name="white_balance" type="select:auto,0|Daylight,1|Fluorescent,2|Tungsten,3|Flash,4|Cloudy,5|Shade,6">select white balance</field>
                <field name="exposure_compensation" type="select:0,15|-5000,0|-4666,1|-4333,2|-4000,3|-3666,4|-3333,5|-3000,6|-2666,7|-2333,8|-2000,9|-1666,10|-1333,11|-1000,12|-666,13|-333,14|333,16|666,17|1000,18|1333,19|1666,20|2000,21|2333,22|2666,23|3000,24|3333,25|3666,26|4000,27|4333,28|4666,29|5000,30">select exposure compensation</field>
                <field name="bracketing" type="select:none|AE,1">select bracketing</field>
                <field name="auto_focus" type="select:off|on">select auto focus</field>
                <field name="focus" type="text" defaultvalue="">manual focus value (0-32767)</field>
                <field name="gphoto2_config" type="text" defaultvalue="">Comma separated configurations for gphoto2</field>
            </command>
            <command name="panorama_picture">
                <field name="core_station" type="*text" defaultvalue="65535">the device_id of the receiving core station</field>
                <field name="camera" type="select:DSLR,1|Webcam,2">select camera</field>
                <field name="start_x" type="text" defaultvalue="0">Lower-left horizontal starting point</field>
                <field name="start_y" type="text" defaultvalue="0">Lower-left vertical starting point</field>
                <field name="pictures_x" type="text" defaultvalue="1">Number of pictures taken horizontally</field>
                <field name="pictures_y" type="text" defaultvalue="1">Number of pictures taken vertically</field>
                <field name="rotation_x" type="text" defaultvalue="">Horizontal rotation in degrees between pictures</field>
                <field name="rotation_y" type="text" defaultvalue="">Vertical rotation in degrees between pictures</field>
                <field name="delay" type="text" defaultvalue="0">Delay between rotation and picture taking</field>
                <field name="batch_download" type="checkbox:batch_download" defaultvalue="1">select if picture download from camera should be done after all pictures have been taken</field>
                <field name="image_quality" type="select:JPEG Basic,0|JPEG Normal,1|JPEG Fine,2|TIFF (RGB),3|NEF (Raw),4|NEF+Basic,5|NEF+Normal,6|NEF+Fine,7">select image quality</field>
                <field name="image_size" type="select:2144x1424,2|3216x2136,1|4288x2848,0">select image size</field>
                <field name="aperture" type="select:auto|f/5.6,0|f/6.3,1|f/7.1,2|f/8,3|f/9,4|f/10,5|f/11,6|f/13,7|f/14,8|f/16,9|f/18,10|f/20,11|f/22,12|f/25,13|f/29,14|f/32,15|f/36,16">select aperture</field>
                <field name="shutter_speed" type="select:auto|0.0001s,0|0.0002s,1|0.0003s,2|0.0004s,3|0.0005s,4|0.0006s,5|0.0008s,6|0.0010s,7|0.0012s,8|0.0015s,9|0.0020s,10|0.0025s,11|0.0031s,12|0.0050s,13|0.0050s,14|0.0062s,15|0.0080s,16|0.0100s,17|0.0125s,18|0.0166s,19|0.0200s,20|0.0250s,21|0.0333s,22|0.0400s,23|0.0500s,24|0.0666s,25|0.0769s,26|0.1000s,27|0.1250s,28|0.1666s,29|0.2000s,30|0.2500s,31|0.3333s,32|0.4000s,33|0.5000s,34|0.6250s,35|0.7692s,36|1.0000s,37|1.300s,38|1.6000s,39|2.0000s,40|2.5000s,41|3.0000s,42|4.0000s,43|5.0000s,44|6.0000s,45|8.0000s,46|10.0000s,47|13.0000s,48|15.0000s,49|20.0000s,50|25.0000s,51|30.0000s,52">select shutter speed</field>
                <field name="iso" type="select:auto|100,0|125,1|160,2|200,3|250,4|320,5|400,6|500,7|640,8|800,9|1000,10|1250,11|1600,12|2000,13|2500,14|3200,15|4000,16|5000,17|6400,18">select ISO</field>
                <field name="white_balance" type="select:auto,0|Daylight,1|Fluorescent,2|Tungsten,3|Flash,4|Cloudy,5|Shade,6">select white balance</field>
                <field name="exposure_compensation" type="select:0,15|-5000,0|-4666,1|-4333,2|-4000,3|-3666,4|-3333,5|-3000,6|-2666,7|-2333,8|-2000,9|-1666,10|-1333,11|-1000,12|-666,13|-333,14|333,16|666,17|1000,18|1333,19|1666,20|2000,21|2333,22|2666,23|3000,24|3333,25|3666,26|4000,27|4333,28|4666,29|5000,30">select exposure compensation</field>
                <field name="bracketing" type="select:none|AE,1">select bracketing</field>
                <field name="auto_focus" type="select:off|on">select auto focus</field>
                <field name="focus" type="text" defaultvalue="">manual focus value (0-32767)</field>
                <field name="gphoto2_config" type="text" defaultvalue="">Comma separated configurations for gphoto2</field>
            </command>
            <command name="positioning">
                <field name="core_station" type="*text" defaultvalue="65535">the device_id of the receiving core station</field>
                <field name="x" type="text" defaultvalue="0">horizontal position in degrees</field>
                <field name="y" type="text" defaultvalue="0">vertical position in degrees</field>
            </command>
            <command name="operating_mode">
                <field name="core_station" type="*text" defaultvalue="65535">the device_id of the receiving core station</field>
                <field name="mode" type="*checkbox:manual">select if manual mode (joystick on) should be set</field>
            </command>
            <command name="calibration">
                <field name="core_station" type="*text" defaultvalue="65535">the device_id of the receiving core station</field>
            </command>
            <command name="power_settings">
                <field name="core_station" type="*text" defaultvalue="65535">the device_id of the receiving core station</field>
                <field name="robot_and_camera" type="*checkbox:power">select if robot and camera power should be turned on</field>
                <field name="heater" type="*checkbox:power">select if heater should be turned on</field>
            </command>
        </web-input>
        <output-structure>
            <field name="DEVICE_ID" type="INTEGER" />
            <field name="GENERATION_TIME" type="BIGINT" unit="unixtime" index="true"/>
            <field name="TIMESTAMP" type="BIGINT" unit="unixtime" />
            
            <field name="COMMAND" type="VARCHAR(127)" />
            <field name="INFO" type="VARCHAR(255)" />
            <field name="X" type="VARCHAR(8)" unit="째" />
            <field name="Y" type="VARCHAR(8)" unit="째" />
            <field name="START_X" type="VARCHAR(8)" unit="째" />
            <field name="START_Y" type="VARCHAR(8)" unit="째" />
            <field name="PICTURES_X" type="SMALLINT" />
            <field name="PICTURES_Y" type="SMALLINT" />
            <field name="ROTATION_X" type="VARCHAR(8)" />
            <field name="ROTATION_Y" type="VARCHAR(8)" />
            <field name="DELAY" type="SMALLINT" unit="s" />
            <field name="BATCH_DOWNLOAD" type="SMALLINT" />
            <field name="GPHOTO2_CONFIG" type="BINARY" />
            
            <field name="DATA_IMPORT_SOURCE" type="SMALLINT" />
        </output-structure>
    </processing-class>
    <description>Show CamZilla information of the CoreStations from the Dirruhorn deployment</description>
    <life-cycle pool-size="10" />
    <addressing />
    <storage />
    <streams>
        <stream name="data2">
            <source alias="source" storage-size="1" sampling-rate="1">
                <address wrapper="backlog">
                    <predicate key="remote-connection">permasense-gg-cam01.ethz.ch:9003</predicate>
                    <predicate key="plugin-classname">gsn.wrappers.backlog.plugins.CamZillaPlugin</predicate>
                </address>
                <query> select * from wrapper </query>
            </source>
            <query> select * from source </query>
        </stream>

        </streams>
</virtual-sensor>
