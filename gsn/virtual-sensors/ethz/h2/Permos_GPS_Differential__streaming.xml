<virtual-sensor name="Permos_GPS_Differential__streaming" priority="10" >
    <processing-class>
        <class-name>gsn.vsensor.BridgeVirtualSensorPermasense</class-name>
        <unique-timestamps>false</unique-timestamps>
        <output-structure>
            <field name="RAW_PACKET" type="BINARY"/>
            
        </output-structure>
    </processing-class>
    <description>Processed GPS data from Permos deployment</description>
    <life-cycle pool-size="10" />
    <addressing />
    <storage user="permasense_pbl" password="CHANGEME" driver="com.mysql.jdbc.Driver" url="jdbc:mysql://lochmatter.ee.ethz.ch/permasense_pbl_backuped?autoReconnect=true&amp;profilerEventHandler=gsn.wrappers.MySQLProfilerEventHandler&amp;profileSQL=true&amp;logSlowQueries=true"/>
    <streams>
        <stream name="data">
            <source alias="source" storage-size="1" sampling-rate="1">
                <address wrapper="tcp">
                    <predicate key="PORT">22502</predicate>
                    <predicate key="buffer-size">1024</predicate>
                </address>
                <query> select * from wrapper </query>
            </source>
            <query> select * from source </query>
        </stream>
    </streams>
</virtual-sensor>
