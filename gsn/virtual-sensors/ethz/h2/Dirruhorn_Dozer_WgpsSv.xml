<virtual-sensor name="Dirruhorn_Dozer_WgpsSv" priority="10" >
    <processing-class>
        <class-name>gsn.vsensor.BridgeVirtualSensorPermasense</class-name>
        <unique-timestamps>false</unique-timestamps>
        <output-structure>
            <field name="DEVICE_ID" type="INTEGER" />
            <field name="GENERATION_TIME" type="BIGINT" unit="unixtime" index="true"/>
            <field name="TIMESTAMP" type="BIGINT" unit="unixtime"/>
            <field name="HEADER_SEQNR" type="INTEGER" />
            <field name="HEADER_ORIGINATORID" type="INTEGER" />
            <field name="HEADER_ATIME" type="INTEGER" unit="s"/>
            <field name="PAYLOAD_GPS_TIME" type="INTEGER" />
            <field name="PAYLOAD_GPS_WEEK" type="SMALLINT" />
            <field name="PAYLOAD_NUM_SV" type="TINYINT" />
            <field name="PAYLOAD_CARRIER_PHASE_I0" type="TINYINT" />
            <field name="PAYLOAD_CARRIER_PHASE_I1" type="TINYINT" />
            <field name="PAYLOAD_CARRIER_PHASE_I2" type="TINYINT" />
            <field name="PAYLOAD_CARRIER_PHASE_I3" type="TINYINT" />
            <field name="PAYLOAD_CARRIER_PHASE_I4" type="TINYINT" />
