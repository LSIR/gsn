            <field name="PAYLOAD_PSEUDO_RANGE_I2" type="TINYINT" />
            <field name="PAYLOAD_PSEUDO_RANGE_I3" type="TINYINT" />
            <field name="PAYLOAD_PSEUDO_RANGE_I4" type="TINYINT" />
            <field name="PAYLOAD_DOPPLER_I0" type="TINYINT" />
            <field name="PAYLOAD_DOPPLER_I1" type="TINYINT" />
            <field name="PAYLOAD_DOPPLER_I2" type="TINYINT" />
            <field name="PAYLOAD_SPACE_VEHICLE" type="TINYINT" />
            <field name="PAYLOAD_MEASUREMENT_QUALITY" type="TINYINT" />
            <field name="PAYLOAD_SIGNAL_STRENGTH" type="TINYINT" />
            <field name="PAYLOAD_LOSS_OF_LOCK" type="TINYINT" />
            <field name="DATA_IMPORT_SOURCE" type="SMALLINT" />
        </output-structure>
    </processing-class>
    <description>Dozer messages (DozerWgpsSvMsg) from Dirruhorn deployment</description>
    <life-cycle pool-size="10" />
    <addressing />
    <storage />
    <streams>
        <stream name="data">
            <source alias="source" storage-size="1" sampling-rate="1">
                <address wrapper="backlog">
                    <predicate key="remote-connection">permasense-dh-bs01.ethz.ch:9003</predicate>
                    <predicate key="plugin-classname">gsn.wrappers.backlog.plugins.MigMessagePlugin</predicate>
                    <predicate key="message-classname">ch.ethz.permasense.tinyos2x.messages.DozerWgpsSvMsg</predicate>
                </address>
                <query> select * from wrapper </query>
            </source>
            <query> select * from source </query>
        </stream>
        <stream name="data0">
