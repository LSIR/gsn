<virtual-sensor name="Dirruhorn_OutbackMate3" priority="10" >
    <processing-class>
        <class-name>gsn.vsensor.BridgeVirtualSensorPermasense</class-name>
        <unique-timestamps>false</unique-timestamps>
       	<output-structure>
            <field name="DEVICE_ID" type="INTEGER" />
			<field name="GENERATION_TIME" type="BIGINT" unit="unixtime" index="true"/>
			<field name="SIZE" type="INTEGER" />
			<field name="DATA" type="binary" />
            <field name="DATA_IMPORT_SOURCE" type="SMALLINT" />
       	</output-structure>
    </processing-class>
    <description>Outback Mate 3 status data for Dirruhorn deployment</description>
    <life-cycle pool-size="10" />
    <addressing />
    <storage />
    <streams>
        <stream name="data1">
            <source alias="source" storage-size="1" sampling-rate="1">
                <address wrapper="http-get">
                    <predicate key="url">http://permasense-rd-mon01.ethz.ch/Dev_status.cgi?Port=0</predicate>
                    <predicate key="rate">120000</predicate>
                    <predicate key="device-id">9000</predicate>
                </address>
                <query> select * from wrapper </query>
            </source>
            <query> select * from source </query>
        </stream>
    </streams>
</virtual-sensor>
