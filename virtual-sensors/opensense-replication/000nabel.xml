<virtual-sensor name="nabel_lausanne" priority="10">	<processing-class>		<class-name>ch.epfl.gsn.vsensor.BridgeVirtualSensor</class-name>		<init-params />		<output-structure>			<field name="O3" type="double"/>			<field name="CO" type="double" />			<field name="NO2" type="double" />			<field name="NO" type="double" />		</output-structure>	</processing-class>	<description>Automatically retreived data from Nabel Station in Lausanne.</description>	<life-cycle pool-size="10" />	<addressing>		<predicate key="geographical">Lausanne-CÃ©sar-Roux</predicate>		<predicate key="LATITUDE">46.52194</predicate>		<predicate key="LONGITUDE">6.63972</predicate>	</addressing>	<storage />	<streams>		<stream name="data">			<source alias="source" storage-size="1" sampling-rate="1">				<address wrapper="csv">					<predicate key="file">/home/nabel/data/LAU_pollutants_MM10.csv</predicate>					<predicate key="fields">timed;O3;CO;NO2;NO</predicate>					<predicate key="formats">timestamp(dd.MM.yy HH:mm);numeric;numeric;numeric</predicate>					<predicate key="separator">;</predicate>					<predicate key="skip-first-lines">2</predicate>				</address>				<query>select * from wrapper</query>			</source>			<query>select * from source</query>		</stream>	</streams></virtual-sensor> 